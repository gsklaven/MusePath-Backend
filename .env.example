# Server Configuration
PORT=3000
NODE_ENV=development

# MongoDB Configuration (Optional - falls back to mock data if not provided)
MONGODB_URI=

# API Configuration
API_VERSION=v1
MAX_REQUEST_SIZE=10mb

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Walking Speed (km/h)
DEFAULT_WALKING_SPEED=5

# Route Deviation Threshold (meters)
ROUTE_DEVIATION_THRESHOLD=50
