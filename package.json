  "keywords": [
    "museum",
    "navigation",
    "express",
    "rest-api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ava": "^6.4.1",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "got": "^14.6.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^7.0.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "tough-cookie": "^6.0.0"
  },
  "devDependencies": {
    "c8": "^10.1.3"
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "timeout": "2m",
    "workerThreads": false
  }
